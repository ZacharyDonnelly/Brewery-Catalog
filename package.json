{
  "name": "brewery-catalog",
  "version": "1.0.0",
  "author": "Zachary Donnelly",
  "license": "MIT",
  "private": true,
  "keywords": [
    "brewery",
    "brewery-catalog",
    "walmart-labs",
    "config",
    "javascript",
    "stylelint",
    "react",
    "jest",
    "grapql",
    "apollo",
    "react-spring"
  ],
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@material-ui/utils": "4.11.3",
    "@mui/icons-material": "5.8.4",
    "apollo-datasource-rest": "^3.6.1",
    "apollo-link-rest": "^0.9.0-rc.1",
    "apollo-server": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql-anywhere": "^4.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-spring": "^9.4.5"
  },
  "scripts": {
    "start": "concurrently 'yarn' 'react-scripts start' 'yarn start:server'",
    "start:server": "cd server && nodemon --config nodemon.json index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:jest": "jest",
    "test:lint": "eslint . && prettier --check .",
    "lint:all": "concurrently 'yarn lint:eslint' 'yarn lint:eslint' 'yarn lint:stylelint'",
    "lint:eslint": "eslint --fix --ignore-path .gitignore --ext  **/*.js, **/*.jsx",
    "lint:prettier": "prettier --write \"**/*.{css,scss}\"",
    "lint:stylelint": "stylelint --fix --config ./.stylelint.config.js \"**/*.{css,scss}\"",
    "clean": " yarn cache clean && rimraf node_modules && yarn install --force"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "chalk": "^5.0.1",
    "classnames": "^2.3.1",
    "concurrently": "^7.2.2",
    "core-js": "^3.23.2",
    "eslint": "7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-hooks": "^4.7.0-next-955cad9bc-20220623",
    "graphql": "^16.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "scriptlint": "^2.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0"
  },
  "engines": {
    "node": ">=16.5.1"
  },
  "hooks": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "peerDependenciesMeta": {
    "eslint-config-prettier": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZacharyDonnelly/Brewery-Catalog.git"
  },
  "scriptlint": {
    "rules": {
      "uses-allowed-namespace": false
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
